    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&mt_exit_AML_on_tap Q TAB               &kp_exit_AML W                  &kp_exit_AML E                  &kp_exit_AML R             &kp_exit_AML T                                                                                         &kp_exit_AML Y                &kp_exit_AML U  &kp_exit_AML I                     &lt_exit_AML 5 O                  &kp_exit_AML P
&kp_exit_AML A                          &kp_exit_AML S                  &kp_exit_AML D                  &kp_exit_AML F             &kp_exit_AML G             &ht_kp_mkp_exit_AML ESCAPE LCLK      &kp_exit_AML MINUS                     &kp_exit_AML H                &kp_exit_AML J  &kp_exit_AML K                     &kp_exit_AML L                    &mt_exit_AML EQUAL ENTER
&mt_exit_AML_on_tap LEFT_SHIFT Z        &kp_exit_AML X                  &kp_exit_AML C                  &kp_exit_AML V             &kp_exit_AML B             &kp_exit_AML UP_ARROW                &mt_exit_AML_on_hold DELETE BACKSPACE  &kp_exit_AML N                &kp_exit_AML M  &mt_exit_AML LA(LEFT_ARROW) COMMA  &mt_exit_AML LA(RIGHT_ARROW) DOT  &mt_exit_AML_on_tap RIGHT_SHIFT SLASH
&mt_exit_AML_on_tap LCTRL LEFT_CONTROL  &mt_exit_AML LEFT_WIN LEFT_GUI  &mt_exit_AML LEFT_ALT LEFT_ALT  &lt_exit_AML 6 LEFT_ARROW  &lt_exit_AML 2 DOWN_ARROW  &lt_exit_AML 3 RIGHT_ARROW           &lt_exit_AML 3 ENTER                   &lt_exit_AML_on_hold 2 SPACE                                                                                       &kp_exit_AML LG(E)
            >;

            sensor-bindings = <&ROTATE SCRL_DOWN SCRL_UP>;
